rule "Turn On Porch Light"
when
    Time cron "0 11 0 * * ?" // 18:00:00, every day, every month, every weekday, don't care about year
then
    Main_Porch_Light.sendCommand(1)
end 

rule "Turn Off Porch Light"
when
    Time cron "0 14 0 * * ?" // 07:00:00, every day, every month, every weekday, don't care about year
then
    Main_Porch_Light.sendCommand(Off)
end 

